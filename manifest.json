{
    "manifest_version": 2,
    
    "name":"Smilence",
    "version":"1.4.0",
    
    "permissions": [
        "tabs",
        "http://self.dgut.edu.cn/"
    ],
    
    "browser_action": {
        "defult_icon":"icon.png",
        "default_title": "Smilence"
    },
    
    "icons": { 
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },  

    "content_scripts": [
        {
          "matches": ["http://self.dgut.edu.cn/Flow/Index"],
          "js": ["scripts.js"]
        }
    ]
}
